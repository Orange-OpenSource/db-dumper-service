#!/usr/bin/env bash
BASEDIR=$(dirname $0)
"${BASEDIR}/mysqldump" --routines --host=$1 --port=$2 --user=$3 --password=$4 $5 > "${BASEDIR}/source.sql"
"${BASEDIR}/mysqldump" --routines --host=$6 --port=$7 --user=$8 --password=$9 ${10} > "${BASEDIR}/source.sql"
diff "${BASEDIR}/source.sql" "${BASEDIR}/source.sql" > /dev/null
OUT=$?
if [ $OUT -eq 0 ];then
    echo "1" > "${BASEDIR}/diff_state.txt"
else
    echo "2" > "${BASEDIR}/diff_state.txt"
fi
